		expect(middlewareRemote).to.equal(asyncRemote)

		asyncRemote:onRequest(function(...)
			arg1, arg2 = ...
			return "result"
		end)

		expect(instance:InvokeClient(player, "test", 1)).to.equal("result!")
		expect(arg1).to.equal("intercepted")
		expect(arg2).to.equal(2)
